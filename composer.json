{
    "name": "96qbhy/open-taobao",
    "description": "淘宝开放平台sdk",
    "keywords": [
        "淘宝",
        "淘宝开放平台",
        "淘宝客"
    ],
    "authors": [
        {
            "name": "qbhy",
            "email": "96qbhy@gmail.com"
        }
    ],
    "require": {
        "php": "7.* | 8.*",
        "hanson/foundation-sdk": "^4.0",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "symfony/var-dumper": "^5.1"
    },
    "autoload": {
        "psr-4": {
            "Qbhy\\OpenTaobao\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OpenTaobao\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Qbhy\\OpenTaobao\\LaravelServiceProvider"
            ]
        }
    }
}
